				{
					"name": "99-01-12 Enable Predict",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code Check\", function () {",
									"    var jsonData = pm.response.json();",
									"    postman.setNextRequest(\"99-01-99 Final\")",
									"    if (jsonData.state != 'success') {",
									"        var varRunErrors = Number(pm.variables.get(\"varRunErrors\") || \"0\")",
									"        varRunErrors += 1;",
									"        pm.variables.set(\"varRunErrors\", varRunErrors);",
									"    }",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Check that VC Testname does not exist\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.vcsetting && jsonData.vcsetting.length >= 0).to.be.eql(true, \"No setting List found\");",
									"    var searchName = \"EnablePredict\";",
									"    var list = jsonData.vcsetting;",
									"    var index = list.findIndex(item => item.name.indexOf(searchName) == 0);",
									"    pm.expect(index >= 0).to.be.eql(true, \"EnablePredict not found\");",
									"    var varPredict = pm.variables.get(\"varPredict\")",
									"    pm.expect(list[index].value.VCEnabled).to.be.eql(varPredict, \"EnablePredict not set as expected\");",
									"});",
									"",
									""
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var predict = pm.environment.get(\"Predict\") == '0' ? false : true;",
									"pm.variables.set(\"varPredict\", predict);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "access-key",
								"value": "{{token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"value\": {\n        \"VCEnabled\": {{varPredict}}\n    }\n}   ",
