						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var baseName = pm.environment.get(\"VCBaseName\");",
									"var vcNewName = pm.iterationData.get(\"name\");",
									"var vcDescription = pm.iterationData.get(\"description\")  + ' vcid:' + vcNewName;",
									"vcDescription = vcDescription.replace(/\\n/g, ' ');",
									"",
