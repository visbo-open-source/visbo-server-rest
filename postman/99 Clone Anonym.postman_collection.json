									"",
									"pm.variables.set(\"vcNewName\", 'Anno-' + baseName + ' ' + vcNewName);",
									"pm.variables.set(\"vcDescription\", vcDescription);",
									"",
									"var list = [];",
									"pm.globals.set(\"vcAnnoList\", JSON.stringify(list));",
									""
								],
								"type": "text/javascript"
							}
						}
