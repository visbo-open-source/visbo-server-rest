{
  "name": "visbo-rest-server",
  "author": "Visbo GmbH",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "audit": "npm audit | tee test-rest-audit-log.txt; echo Executed on $(date) >> test-rest-audit-log.txt",
    "auditWin": "npm audit > test-rest-audit-log.txt & echo Executed on %date% %time% >> test-rest-audit-log.txt & type test-rest-audit-log.txt",
    "test": "bash testPostman.sh | tee test-rest-postman-log.txt",
    "testWin": "testPostman.bat > test-rest-postman-log.txt & type test-rest-postman-log.txt",
    "testMac": "bash testPostman.sh > test-rest-postman-log.txt && cat test-rest-postman-log.txt",
    "testWin100": "testPostman100.bat > test-rest-postman100-log.txt & type test-rest-postman100-log.txt",
    "lint": "eslint --ignore-path .gitignore .",
    "maillint": "ejslint emailTemplates ",
    "docker:network": "docker network create -d bridge redis_network",
    "docker:redis": "docker run --name redis -d --network redis_network -p 6379:6379 redis",
    "docker:rest-up": "docker build -f Dockerfile -t visbo-rest ."
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "bcrypt": "^6.0.0",
    "bson": "^4.5.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "debug": "^4.2.0",
    "delay": "^5.0.0",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.5",
    "express": "^5.1.0",
    "i18n": "^0.14.2",
    "jsonwebtoken": "^9.0.3",
    "log4js": "^6.9.1",
    "mongoose": "^6.0.0",
    "morgan": "^1.10.1",
    "nodemailer": "^6.10.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "q": "^1.5.0",
    "redis": "^4.7.1",
    "sanitize-html": "^2.17.0",
    "serve-favicon": "^2.5.1",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "ejs-lint": "^2.0.0",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.11",
    "npm": "^10.9.4"
  }
}
